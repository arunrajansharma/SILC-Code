decl 
integer fun(integer a);
integer funmore(integer b);
enddecl

integer fun(integer a) {
begin 
return a*2;
end 
}

integer funmore(integer b)
{
 begin 
return a*5;
end
}

integer main () {
decl
integer result ;
integer resultmore ;

enddecl

begin 
result = fun(3);
resultmore =  fun (5);

return 1;
end
}
